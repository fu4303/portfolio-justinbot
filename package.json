  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.11.6",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "babel-loader": "^8.1.0",
    "bulma": "^0.9.0",
    "bulmaswatch": "^0.8.1",
    "css-loader": "^4.3.0",
    "js-yaml": "^3.14.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
