{
  "name": "justincarlson-io",
  "version": "1.0.0",
  "description": "https://justincarlson.io",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:assets": "webpack --config webpack.config.prod.js",
    "build:site": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "build": "npm run clean && npm run build:assets && npm run build:site",
    "dev:assets": "webpack --config webpack.config.dev.js",
    "dev:site": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "dev": "npm run clean && npm run dev:assets & npm run dev:site"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/justinbot/justincarlson-io.git"
  },
